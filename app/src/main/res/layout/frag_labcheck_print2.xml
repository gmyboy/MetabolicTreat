<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <include layout="@layout/include_frag_bottom" />

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentTop="true"
        android:layout_marginBottom="@dimen/content_margin_bottom">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="10dp"
                android:background="@color/grey_line"
                android:orientation="horizontal">

                <View style="@style/AllLineVertical" />

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="vertical">

                    <View style="@style/LineGreyHorizontal" />

                    <LinearLayout
                        style="@style/Style_Item_Layout"
                        android:weightSum="3">

                        <TextView
                            style="@style/ItemRecordTextView"
                            android:layout_weight="1"
                            android:text="甲状腺功能及抗体" />

                        <LinearLayout
                            style="@style/ItemEditLayout"
                            android:layout_marginLeft="@dimen/horizontal_lay"
                            android:layout_marginRight="@dimen/horizontal_lay"
                            android:layout_weight="1">

                            <TextView
                                android:id="@+id/et_lab2_date1_1"
                                style="@style/CommonItemLayoutEdit"
                                android:layout_weight="1"
                                android:hint="@string/hint_date" />
                        </LinearLayout>
                    </LinearLayout>

                    <View style="@style/LineGreyHorizontal" />

                    <LinearLayout style="@style/Style_Item_Layout">

                        <TextView
                            style="@style/ItemRecordTextView"
                            android:layout_weight="1"
                            android:text="FT3" />

                        <LinearLayout
                            style="@style/ItemEditLayout"
                            android:layout_marginRight="@dimen/horizontal_lay">

                            <TextView
                                android:id="@+id/et_lab2_1"
                                style="@style/ItemLayoutEdit"
                                android:layout_weight="1.5" />

                            <TextView
                                style="@style/TipTextView_Smarl"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:text="Pmol/L" />
                        </LinearLayout>

                        <TextView
                            style="@style/ItemRecordTextView"
                            android:layout_weight="1"
                            android:text="FT4" />

                        <LinearLayout
                            style="@style/ItemEditLayout"
                            android:layout_marginRight="@dimen/horizontal_lay">

                            <TextView
                                android:id="@+id/et_lab2_2"
                                style="@style/ItemLayoutEdit"
                                android:layout_weight="1.5" />

                            <TextView
                                style="@style/TipTextView_Smarl"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:text="PmoI/L" />
                        </LinearLayout>

                        <TextView
                            style="@style/ItemRecordTextView"
                            android:layout_weight="1"
                            android:text="TSH" />

                        <LinearLayout style="@style/ItemEditLayout">

                            <TextView
                                android:id="@+id/et_lab2_3"
                                style="@style/ItemLayoutEdit"
                                android:layout_weight="1.5" />

                            <TextView
                                style="@style/TipTextView"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:text="MIU/L" />
                        </LinearLayout>
                    </LinearLayout>

                    <View style="@style/LineGreyHorizontal" />

                    <LinearLayout style="@style/Style_Item_Layout">

                        <TextView
                            style="@style/ItemRecordTextView"
                            android:layout_weight="1"
                            android:text="TPOAb" />

                        <LinearLayout
                            style="@style/ItemEditLayout"
                            android:layout_marginRight="@dimen/horizontal_lay">

                            <TextView
                                android:id="@+id/et_lab2_4"
                                style="@style/ItemLayoutEdit"
                                android:layout_weight="1.5" />

                            <TextView
                                style="@style/TipTextView"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:text="IU/ML" />
                        </LinearLayout>

                        <TextView
                            style="@style/ItemRecordTextView"
                            android:layout_weight="1"
                            android:text="TGAb" />

                        <LinearLayout
                            style="@style/ItemEditLayout"
                            android:layout_marginRight="@dimen/horizontal_lay">

                            <TextView
                                android:id="@+id/et_lab2_5"
                                style="@style/ItemLayoutEdit"
                                android:layout_weight="1.5" />

                            <TextView
                                style="@style/TipTextView"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:text="IU/ML" />
                        </LinearLayout>

                        <TextView
                            style="@style/ItemRecordTextView"
                            android:layout_weight="1"
                            android:text="TRAb" />

                        <LinearLayout style="@style/ItemEditLayout">

                            <TextView
                                android:id="@+id/et_lab2_6"
                                style="@style/ItemLayoutEdit"
                                android:layout_weight="1.5" />

                            <TextView
                                style="@style/TipTextView"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:text="IU/L" />
                        </LinearLayout>
                    </LinearLayout>

                    <View style="@style/LineGreyHorizontal" />
                </LinearLayout>

                <View
                    style="@style/AllLineVertical"
                    android:layout_gravity="right" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="10dp"
                android:background="@color/grey_line"
                android:orientation="horizontal">

                <View style="@style/AllLineVertical" />

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="vertical">

                    <View style="@style/LineGreyHorizontal" />

                    <LinearLayout
                        style="@style/Style_Item_Layout"
                        android:weightSum="3">

                        <TextView
                            style="@style/ItemRecordTextView"
                            android:layout_weight="1"
                            android:text="血白细胞" />

                        <LinearLayout
                            style="@style/ItemEditLayout"
                            android:layout_marginLeft="@dimen/horizontal_lay"
                            android:layout_marginRight="@dimen/horizontal_lay"
                            android:layout_weight="1">

                            <TextView
                                android:id="@+id/et_lab2_date2_1"
                                style="@style/CommonItemLayoutEdit"
                                android:layout_weight="1"
                                android:hint="@string/hint_date" />
                        </LinearLayout>
                    </LinearLayout>

                    <View style="@style/LineGreyHorizontal" />

                    <LinearLayout style="@style/Style_Item_Layout">

                        <TextView
                            style="@style/ItemRecordTextView"
                            android:layout_weight="1"
                            android:text="WBC" />

                        <LinearLayout
                            style="@style/ItemEditLayout"
                            android:layout_marginRight="@dimen/horizontal_lay">

                            <TextView
                                android:id="@+id/et_lab2_7"
                                style="@style/ItemLayoutEdit"
                                android:layout_weight="1.5" />

                            <TextView
                                style="@style/TipTextView"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:text="×10^9/L"
                                android:textSize="16sp" />
                        </LinearLayout>

                        <TextView
                            style="@style/ItemRecordTextView"
                            android:layout_weight="1"
                            android:text="中性细胞%" />

                        <LinearLayout
                            style="@style/ItemEditLayout"
                            android:layout_marginRight="@dimen/horizontal_lay">

                            <TextView
                                android:id="@+id/et_lab2_8"
                                style="@style/ItemLayoutEdit"
                                android:layout_weight="1.5" />

                            <TextView
                                style="@style/TipTextView"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:text="%" />
                        </LinearLayout>

                        <TextView
                            style="@style/ItemRecordTextView"
                            android:layout_weight="1"
                            android:text="中性细胞绝对值"
                            android:textSize="14sp" />

                        <LinearLayout style="@style/ItemEditLayout">

                            <TextView
                                android:id="@+id/et_lab2_9"
                                style="@style/ItemLayoutEdit"
                                android:layout_weight="1.5" />

                            <TextView
                                style="@style/TipTextView"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
