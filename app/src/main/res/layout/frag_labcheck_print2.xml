                    android:layout_gravity="right" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="10dp"
                android:background="@color/grey_line"
                android:orientation="horizontal">

                <View style="@style/AllLineVertical" />

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="vertical">

                    <View style="@style/LineGreyHorizontal" />

                    <LinearLayout
                        style="@style/Style_Item_Layout"
                        android:weightSum="3">

                        <TextView
                            style="@style/ItemRecordTextView"
                            android:layout_weight="1"
                            android:text="肝功能" />

                        <LinearLayout
                            style="@style/ItemEditLayout"
                            android:layout_marginLeft="@dimen/horizontal_lay"
                            android:layout_marginRight="@dimen/horizontal_lay"
                            android:layout_weight="1">

                            <TextView
                                android:id="@+id/et_lab2_date3_1"
                                style="@style/CommonItemLayoutEdit"
                                android:layout_weight="1"
                                android:hint="@string/hint_date" />
                        </LinearLayout>
                    </LinearLayout>

                    <View style="@style/LineGreyHorizontal" />

                    <LinearLayout style="@style/Style_Item_Layout">

                        <TextView
                            style="@style/ItemRecordTextView"
                            android:layout_weight="1"
                            android:text="ALP" />

                        <LinearLayout
                            style="@style/ItemEditLayout"
                            android:layout_marginRight="@dimen/horizontal_lay">

                            <TextView
                                android:id="@+id/et_lab2_10"
                                style="@style/ItemLayoutEdit"
                                android:layout_weight="1.5" />

                            <TextView
                                style="@style/TipTextView"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:text="U/L" />
                        </LinearLayout>

                        <TextView
                            style="@style/ItemRecordTextView"
                            android:layout_weight="1"
                            android:text="ASP" />

                        <LinearLayout
                            style="@style/ItemEditLayout"
                            android:layout_marginRight="@dimen/horizontal_lay">

                            <TextView
                                android:id="@+id/et_lab2_11"
                                style="@style/ItemLayoutEdit"
                                android:layout_weight="1.5" />

                            <TextView
                                style="@style/TipTextView"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:text="U/L" />
                        </LinearLayout>

                        <TextView
                            style="@style/ItemRecordTextView"
                            android:layout_weight="1"
                            android:text="总胆红素" />

                        <LinearLayout style="@style/ItemEditLayout">

                            <TextView
                                android:id="@+id/et_lab2_12"
                                style="@style/ItemLayoutEdit"
                                android:layout_weight="1.5" />

                            <TextView
                                style="@style/TipTextView_Smarl"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:text="UmoI/L" />
                        </LinearLayout>
                    </LinearLayout>

                    <View style="@style/LineGreyHorizontal" />
                </LinearLayout>

                <View
                    style="@style/AllLineVertical"
                    android:layout_gravity="right" />
            </LinearLayout>

        </LinearLayout>
    </ScrollView>
</RelativeLayout>
